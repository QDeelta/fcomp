(def (if b x y)
  (case b
    (False y)
    (True  x)))

(def (take n l)
  (case l
    (Nil Nil)
    ((Cons h t) (if (=? 0 n) Nil (Cons h (take (- n 1) t))))))

(def (zipWith f l1 l2)
  (case l1
    (Nil Nil)
    ((Cons h1 t1) 
      (case l2
        (Nil Nil)
        ((Cons h2 t2) (Cons (f h1 h2) (zipWith f t1 t2)))))))

(def (tail l)
  (case l
    (Nil Nil)
    ((Cons h t) t)))

(def fibs (Cons 1 (Cons 1 (zipWith + fibs (tail fibs)))))

(def (main n) (take n fibs))
